{
  "compilerOptions": {
    "baseUrl": "./",
    "target": "esnext",
    "module": "commonjs",
    "moduleResolution": "node",
    "lib": [
      "esnext",
      "esnext.asynciterable",

    ],
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": false,
    "noEmit": true,
    "resolveJsonModule": true,
    "paths": {
      "~/*": ["../*"],
      "@/*": ["../*"],
      "~~/*": ["../*"],
      "@@/*": ["../*"]
    },
    "types": [
      "@types/node"
    ],
    "outDir": "../dist/",
  },
  "exclude": ["node_modules", ".nuxt", "dist"],
}
